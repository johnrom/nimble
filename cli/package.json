{
  "name": "nmbl-cli",
  "description": "A Nmbl Dev Workflow",
  "version": "1.0.0-alpha1",
  "author": "John Rom @johnrom",
  "bin": {
    "nmbl": "./bin/run"
  },
  "bugs": "https://github.com/johnrom/nmbl-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.7.0",
    "@oclif/config": "^1.16.0",
    "@oclif/plugin-help": "^3.1.0",
    "@types/inquirer": "^7.3.0",
    "debug": "^4.1.1",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "shelljs": "^0.8.4",
    "tslib": "^1.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.27",
    "@types/shelljs": "^0.8.8",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/johnrom/nmbl-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nmbl",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "johnrom/nmbl-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
